{
  "name": "e-far",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:nuclear": "node scripts/nuclear-dev.js",
    "dev:fresh": "node scripts/dev-with-cache-bust.js",
    "dev:no-cache": "vite --force --host",
    "build": "vite build",
    "build:debug": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "cache:clear": "if exist node_modules\\.vite rmdir /s /q node_modules\\.vite && if exist dist rmdir /s /q dist",
    "debug:css": "npm run dev:no-cache",
    "nuclear": "npm run cache:clear && npm run dev:nuclear"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "framer-motion": "^11.18.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.3",
    "vite": "^7.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14"
    
  }
}
